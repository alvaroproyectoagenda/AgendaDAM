<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.proyecto.agendadam">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AgendaDAM">
        <activity
            android:name=".agenda.FormularioAgendaActivity"
            android:parentActivityName=".agenda.AgendaActivity"
            android:exported="true" />
        <activity
            android:name=".agenda.AgendaActivity"
            android:exported="true" />
        <activity
            android:name=".acceso.RegistroActivity"
            android:exported="false"
            android:parentActivityName=".acceso.LoginActivity" />
        <activity
            android:name=".acceso.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>